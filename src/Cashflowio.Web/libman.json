{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [
    {
      "library": "node_modules/admin-lte/plugins/jquery/",
      "files": [ "jquery.min.js" ],
      "destination": "wwwroot/lib/jquery"
    },
    {
      "library": "node_modules/admin-lte/plugins/bootstrap/js/",
      "files": [ "bootstrap.bundle.min.js" ],
      "destination": "wwwroot/lib/bootstrap"
    },
    {
      "library": "node_modules/admin-lte/plugins/fontawesome-free/css/",
      "files": [ "all.min.css" ],
      "destination": "wwwroot/lib/fontawesome"
    },
    {
      "library": "node_modules/admin-lte/plugins/fontawesome-free/webfonts/",
      "destination": "wwwroot/lib/webfonts"
    },
    {
      "library": "node_modules/admin-lte/dist/js/",
      "files": [ "adminlte.min.js" ],
      "destination": "wwwroot/lib/admin-lte"
    },
    {
      "library": "node_modules/admin-lte/dist/css/",
      "files": [ "adminlte.min.css" ],
      "destination": "wwwroot/lib/admin-lte"
    }
  ]
}