@using Cashflowio.Core.Entities
@model IEnumerable<Cashflowio.Core.Entities.RawTransaction>
@{
    ViewData["Title"] = "Raw Transaction List";
}

<ejs-grid id="Grid" dataSource="@Model" allowPaging="true" allowSorting="true" allowFiltering="true">
    <e-grid-pagesettings pageSize="50"></e-grid-pagesettings>
    <e-grid-columns>
        <e-grid-column field="@nameof(RawTransaction.Amount)" headerText="Amount" width="150"></e-grid-column>
    </e-grid-columns>
</ejs-grid>